{% if avatars %}
        {% for avatar in avatars %}
            <img src="{{asset('images/avatar/' ~ avatar.avatar)}}" style="width:45px;height:45px" alt="avatar" class="avatar-image">
        {% endfor %}
        {% else  %}
            <img src="{{asset('images/avatar/image_profile_default.png')}}" alt="avatar" class="avatar-image">
        {% endif %}